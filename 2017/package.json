{
  "name": "adventofcode2017",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Jon Eubank",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-config-airbnb-base": "^12.1.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "func-names": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0
    }
  }
}
